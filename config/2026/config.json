  "sections": [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 19999
        },
        {
          "title": "Starting Alliance",
          "description": "Starting Alliance",
          "type": "select",
          "required": false,
          "code": "startAlliance",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3",
            "NS": "No Show",
            "NA": "N/A"
          }
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Scouter Name",
          "description": "Enter the name of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot",
          "type": "select",
          "required": true,
          "code": "startPosition",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Outpost Side",
            "R2": "Middle",
            "R3": "Depot Side",
            "NS": "No Show"
          }
        },
        {
          "title": "Intake Type",
          "type": "select",
          "required": false,
          "code": "intakeType",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "FI": "Floor Intake",
            "OB": "Over Bumper Intake",
            "HP": "Human Player",
            "NO": "No Intake"
          }
        }
      ]
    },
    {
