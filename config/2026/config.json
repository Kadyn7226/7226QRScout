          "type": "select",
          "required": false,
          "code": "climbPositionAuto",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L": "Left",
            "M": "Middle",
            "R": "Right"
