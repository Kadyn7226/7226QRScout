            "No": "Didn't Climb",
            "L1": "Level 1",
            "L2": "Level 2",
            "L3": "Level 3",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Attempted Climb Position",
          "description": "Where left to right the robot climbed",
          "type": "select",
          "required": false,
          "code": "climbPosition",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L": "Left",
            "M": "Middle",
            "R": "Right"
          }
